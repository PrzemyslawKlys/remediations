<?xml version="1.0"?>
<SettingsLocationTemplate xmlns="http://schemas.microsoft.com/UserExperienceVirtualization/2013A/SettingsLocationTemplate">
  <Name>Adobe Acrobat DC </Name>
  <ID>AdobeAcrobatDC-Acrobatv22</ID>
  <Version>1</Version>
  <Author>
    <Name>Aaron Parker</Name>
    <Email>aaron@stealthpuppy.com</Email>
  </Author>
  <Processes>
    <Process>
      <Filename>Acrobat.exe</Filename>
    </Process>
  </Processes>
  <Settings>
    <Registry>
      <Path Recursive="true">Software\Adobe</Path>
    </Registry>
    <File>
      <Root>
        <EnvironmentVariable>APPDATA</EnvironmentVariable>
      </Root>
      <Path Recursive="true">Adobe\Acrobat\DC</Path>
    </File>
  </Settings>
</SettingsLocationTemplate>